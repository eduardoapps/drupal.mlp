				'not null' => TRUE,
				'default' => -1,
			),

			'group_status' => array (
				'description' => 'Group status (closed, open, terminated)',
				'type' => 'int',
				'not null' => TRUE,
				'default' => -1,
			),

			'closure_date' => array (
				'description' => 'The group will not close before this day.',
				'mysql_type' => 'datetime',
				'type' => 'varchar',
				'not null' => TRUE,
			),

			'gift_date' => array (
				'description' => 'The date on which the gifts will be given.',
				'type' => 'varchar',
				'mysql_type' => 'datetime',
				'not null' => TRUE,
			),
		),

		'primary key' => array ('group_id'),
	);
	}

	if (!db_table_exists ('chss_shuffle')) {
		$schema ['chss_shuffle'] = array (
			'description' => 'The actual order the gifts\' recipient and senders is stored here',
			'fields' => array (
				'group_id' => array (
					'type' => 'int',
					'not null' => TRUE,
