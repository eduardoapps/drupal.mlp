	}

	if (!db_table_exists ('chss_book_hashes')) {

	$schema ['chss_group_hashes'] = array (
		'description' => 'Automatically generated one-time hashes for group introduction.',
		'fields' => array (
			'group_id' => array (
				'description' => 'The group ID this hash is pertinent to.',
