function chss_schema () {
	if (!db_table_exists ('chss_fb_tokens')) {

	$schema ['chss_fb_tokens'] = array (
                'fields' => array (
                        'uid' => array (
                                'type' => 'int',
                                'unsigned' => TRUE,
                                'not null' => TRUE,
                        ),

                        'access_token' => array (
                                'type' => 'varchar',
                                'length' => '255',
                                'not null' => TRUE,
                        ),
                ),

		'primary key' => array ('uid'),
        );
	}

	if (!db_table_exists ('chss_book_db')) {

	$schema ['chss_book_db'] = array (
		'description' => 'The book database.',
		'fields' => array (
			'book_id' => array (
				'description' => 'Book ID.',
				'type' => 'serial',
				'not null' => TRUE,
			),

			'responsible' => array (
				'description' => 'Person responsible (at their shop) for this book.',
				'type' => 'varchar',
				'length' => 255,
			),

			'title' => array (
				'description' => 'Book title.',
				'type' => 'varchar',
				'length' => 255,
			),

			'author' => array (
				'description' => 'Author of the book',
				'type' => 'varchar',
				'length' => 255,
			),

			'isbn' => array (
				'description' => 'ISBN.',
				'type' => 'varchar',
				'length' => 100,
			),

			'age_min' => array (
				'description' => 'Minimum reader age.',
				'type' => 'int',
			),

			'age_max' => array (
				'description' => 'Maximum reader age (0 - 101).',
				'type' => 'int',
			),

			'genre' => array (
				'description' => 'Book genre',
				'type' => 'int',
			),

			'category' => array (
				'description' => 'Category',
				'type' => 'varchar',
				'length' => 255,
			),

			'codewords' => array (
				'description' => 'Code words',
				'type' => 'varchar',
				'length' => 255,
			),

			'phrase' => array (
				'description' => 'Phrase',
				'type' => 'varchar',
				'length' => 255,
			),

			'url' => array (
				'description' => 'URL at the bookstore',
				'type' => 'varchar',
				'length' => 1024,
			),

			'price' => array (
				'description' => 'Price of the book.',
				'type' => 'int',
			),
		),

		'primary key' => array ('book_id'),
	);
	}

	if (!db_table_exists ('chss_book_db_answers')) {

	$schema ['chss_book_db_answers'] = array (
		'desription' => 'The corresponding answers for the book',
		'fields' => array (
			'book_id' => array (
				'description' => 'Book ID',
				'type' => 'int',
				'not null' => TRUE,
			),

			'question_id' => array (
				'description' => 'This selected',
				'type' => 'int',
				'not null' => TRUE,
			),

			'answer' => array (
				'description' => 'Answer for this question related to this book',
				'type' => 'int',
				'not null' => TRUE,
			),
		),
	);
	}

	if (!db_table_exists ('chss_anketa')) {

	$schema['chss_anketa'] = array (
		'description' => 'The answers from the questionnaire.',
		'fields' => array (
			'user_id' => array (
				'description' => 'User id.',
				'type' => 'int',
				'not null' => TRUE,
